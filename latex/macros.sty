\ExplSyntaxOn
\NewDocumentCommand\hrefCIP{m}{%
  \clist_if_in:nnTF {0069,1694} {#1}
  {\href{\detokenize{https://cips.cardano.org/cip/CIP-#1}}{CIP-#1}}%
  {\msg_warning:nnn {agda-latex-macros} {invalid-cip} {#1}
